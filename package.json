{
  "name": "descript-to-video-ui",
  "version": "0.0.1",
  "description": "markdownなどのドキュメントファイルから読み上げ動画を作るやつのUI部分",
  "main": "resources/main.js",
  "scripts": {
    "dev": "shadow-cljs watch main renderer",
    "build": "shadow-cljs compile main renderer",
    "clean": "rm -rf resources/public/js/* && rm -rf target",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [
    "shadow-cljs",
    "reagent",
    "electron"
  ],
  "author": "simana <tktossi@live.com>",
  "build": {
    "appId": "com.electron.descript-to-video.ui",
    "copyright": "Copyright © 2021 ${author}",
    "win": {
      "target": "portable"
    },
    "files": [
      "resources/**/*",
      ".shadow-cljs/**/*"
    ]
  },
  "license": "Eclipse Public License - v 2.0",
  "devDependencies": {
    "electron": "^15.3.1",
    "electron-builder": "^22.14.5",
    "shadow-cljs": "^2.8.110"
  },
  "dependencies": {
    "react": "16.13.0",
    "react-dom": "16.13.0"
  }
}
